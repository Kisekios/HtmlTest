---
import CardLaunch from '../components/CardLaunch.astro';
import { getLatesLaunches } from "../services/spacex";
const launches = await getLatesLaunches()
---

<dir class="grid gap-8 sm:grid-cols-2 lg:grid-cols-2 lg:grid-col-3">
    {
        launches.map(({id, links, details, flight_number:flightNumber, success}) => (
            <CardLaunch
            id={id}
            img={links.patch.small}
            details={details}
            flightNumber={flightNumber}
            success={success}
            />
        ))
    }
</dir>
